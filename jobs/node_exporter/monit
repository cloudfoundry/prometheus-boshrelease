check process node_exporter
  with pidfile /var/vcap/sys/run/node_exporter/node_exporter.pid
  start program "/var/vcap/jobs/node_exporter/bin/monit_debugger node_exporter_ctl '/var/vcap/jobs/node_exporter/bin/node_exporter_ctl start'"
  stop program "/var/vcap/jobs/node_exporter/bin/monit_debugger node_exporter_ctl '/var/vcap/jobs/node_exporter/bin/node_exporter_ctl stop'"
  group vcap
